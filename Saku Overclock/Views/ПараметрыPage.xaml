<Page
    x:Class="Saku_Overclock.Views.ПараметрыPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:local="using:Saku_Overclock.Helpers" xmlns:local1="using:Saku_Overclock.ViewModels"
    mc:Ignorable="d">
    <Page.Resources>
        <ThemeShadow x:Name="SharedShadow"></ThemeShadow>
        <Flyout x:Name="SymbolFlyout" Placement="Bottom">
            <!-- flyout content -->
            <GridView ItemClick="SymbolList_ItemClick" IsItemClickEnabled="True">
                <GridView.ItemsPanel>
                    <ItemsPanelTemplate>
                        <ItemsWrapGrid MaximumRowsOrColumns="5" Orientation="Horizontal"/>
                    </ItemsPanelTemplate>
                </GridView.ItemsPanel>
                <GridView.Resources>
                    <Style TargetType="Rectangle">
                        <Setter Property="Width" Value="40"/>
                        <Setter Property="Height" Value="40"/>
                        <Setter Property="RadiusX" Value="4"/>
                        <Setter Property="RadiusY" Value="4"/>
                    </Style>
                </GridView.Resources>
                <GridView.Items>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xEcad;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xeafc;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xeb05;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xeadf;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xe734;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xeadf;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xeb3c;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xeb44;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xeb51;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xebd2;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xec32;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xec46;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xec48;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xec49;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xec4a;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xecc6;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xe7ba;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xedad;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xee6f;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xee71;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xf211;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xe945;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xe9d9;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xe9ca;"/>
                        <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" FontFamily="{StaticResource SymbolThemeFontFamily}" HorizontalAlignment="Left"     Glyph="&#xe9ce;"/>
                </GridView.Items>
            </GridView>
        </Flyout>
        <local:FocusToSpinButtonPlacementModeConverter x:Key="FocusToPlacement"/>
    </Page.Resources>
    <Grid x:Name="ContentArea" Margin="-48,0,-28,0">
        <Grid>
            <Grid.RowDefinitions> 
                <RowDefinition></RowDefinition>
                <RowDefinition Height="70"></RowDefinition>
            </Grid.RowDefinitions>
            <!-- Начало -->
            <TextBlock x:Uid="Param_Name" Text="Параметры процессора" FontSize="30" Margin="0,-65,0,0"></TextBlock>
            <TextBlock HorizontalAlignment="Right" x:Uid="Param_Preset" Text="Сохранённые пресеты:" FontSize="20" Margin="0,-56,220,0"></TextBlock>
            <Border CornerRadius="4" VerticalAlignment="Top" Height="30" Width="200" Margin="0,-55,-11,0" HorizontalAlignment="Right" Shadow="{StaticResource SharedShadow}" Translation="0,0,20"/>
            <ComboBox CornerRadius="4" HorizontalAlignment="Right" Style="{ThemeResource AcrylicComboBoxStyle}" Height="30" Width="200" Margin="0,-55,-11,0" x:Name="ProfileCOM" SelectionChanged="ProfileCOM_SelectionChanged">
                <ComboBox x:Uid="Param_PremadedCombo" IsEnabled="False"></ComboBox> 
            </ComboBox>

            <!-- Середина -->
            
            <ScrollViewer Grid.Row="0" Padding="0,0,10,0" IsTabStop="False"
         UseSystemFocusVisuals="False" x:Name="MainScroll"
         VerticalScrollBarVisibility="Auto" 
         VerticalScrollMode="Auto" Margin="0,-10,-22,0">
                <Grid VerticalAlignment="Top" x:Name="Param_Func_Grid">
                    <Grid.RowDefinitions>
                        <!-- От 0 до 6 -->
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                    </Grid.RowDefinitions>
                    <Expander Grid.Row="0" IsExpanded="False" VerticalAlignment="Top">
                        <Expander.Header>
                            <Grid Height="65" Width="2000">
                                <SymbolIcon Symbol="SelectAll" HorizontalAlignment="Left" Margin="0,0,9,0"/>
                                <TextBlock Margin="35,12,0,0" FontWeight="SemiBold" VerticalAlignment="Top" x:Uid="Param_CPU_Text"  Text="Мощность процессора"></TextBlock>
                                <TextBlock x:Name="TextColor" Margin="35,30,0,0" FontWeight="Light" VerticalAlignment="Top" x:Uid="Param_CPU_Pdesc"  Text="Мощность процессора"></TextBlock>
                            </Grid>
                        </Expander.Header>
                        <Grid HorizontalAlignment="Left" Margin=" 30,0,0,0" Width="2000">
                            <!-- Монтируем секции -->
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <!-- От 0 до 6 -->
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>

                            <!-- Первая секция -->
                            <StackPanel Padding="0,0,0,10" Orientation="Vertical" Grid.Row="0" Grid.Column="0" HorizontalAlignment="Left" Width="2000">
                                <StackPanel Orientation="Horizontal">
                                    <Border Height="20" Width="20" CornerRadius="15" Margin="0,0,0,0">
                                        <FontIcon Glyph="&#xE9CA;" FontSize="15" >
                                        </FontIcon>
                                        <Border.Background>
                                            <LinearGradientBrush StartPoint="0.3,0.5" EndPoint="1.3,1.5">
                                                <GradientStop Color="#AAADD8E6" Offset="0.0" />
                                                <GradientStop Color="#AAFFC0CB" Offset="0.10" />
                                            </LinearGradientBrush>
                                        </Border.Background>
                                    </Border>
                                    <TextBlock x:Uid="Param_CPU_c1" Margin="5,0,0,0" Text="Максимальная температура CPU" FontSize="15"></TextBlock>
                                    <FontIcon FontFamily="{StaticResource SymbolThemeFontFamily}" x:Uid="Param_c1_Help" HorizontalAlignment="Left" Margin="5,4,0,-4" Glyph="&#xe9ce;">
                                        <FontIcon.RenderTransform>
                                            <ScaleTransform ScaleX="0.7" ScaleY="0.7" />
                                        </FontIcon.RenderTransform>
                                    </FontIcon>
                                </StackPanel>
                                <StackPanel Margin="0,10,0,0" Orientation="Horizontal" HorizontalAlignment="Left">
                                    <Grid>
                                        <CheckBox Width="10" x:Name="c1" Checked="C1_Checked" Unchecked="C1_Checked"></CheckBox>
                                        <Slider x:Name="c1v" ValueChanged="C1_ValueChanged" Margin="35,0,0,0" VerticalAlignment="Top" Width="200" Value="80" HorizontalAlignment="Left" Minimum="50" Maximum="100"/>
                                    </Grid>
                                    <NumberBox Margin="10,0,0,0" Minimum="50" Height="33" Width="120" HorizontalAlignment="Left" SpinButtonPlacementMode="Inline" SmallChange="1" LargeChange="10" x:Name="c1t" ValueChanged="C2t_ValueChanged" GotFocus="C2t_FocusEngaged" LostFocus="C2t_FocusDisengaged" Value="{Binding ElementName=c1v, Path=Value, Mode=TwoWay}"></NumberBox>
                                </StackPanel> 
                            </StackPanel>
                            <Grid Grid.Row="0" Grid.Column="1" HorizontalAlignment="Left" Margin="-600,0,0,0">
                                <TextBlock x:Uid="Param_CPU_Recommend" Text="Рекомендации:        Баланс          Производительность" Foreground="#9db1cc" Grid.Column="1" ></TextBlock>
                                <TextBlock Text="                                    75(С)                         90(С)" Grid.Column="1" Margin="0,20,0,0" ></TextBlock>
                            </Grid>
                            <Grid Grid.Row="1" x:Name="Laptops_HTC_Temp" Grid.Column="0" HorizontalAlignment="Left" Width="2000">
                                <StackPanel Orientation="Horizontal"> 
                                    <TextBlock x:Uid="Param_CPU_c7"  Text="Максимальная температура cHTC (C)" FontSize="15"></TextBlock>
                                    <Border Height="20" Width="20" CornerRadius="15" Margin="3,-30,0,0">
                                        <FontIcon Glyph="&#xE9CA;" FontSize="15" >
                                        </FontIcon>
                                        <Border.Background>
                                            <LinearGradientBrush StartPoint="0.3,0.5" EndPoint="1.3,1.5">
                                                <GradientStop Color="#AAADD8E6" Offset="0.0" />
                                                <GradientStop Color="#AAFFC0CB" Offset="0.10" />
                                            </LinearGradientBrush>
                                        </Border.Background>
                                    </Border> 
                                </StackPanel>
                                <CheckBox  Margin="0,20,0,0" x:Name="c7" Checked="C7_Checked" Unchecked="C7_Checked"></CheckBox>
                                <Slider x:Name="c7v" ValueChanged="C7_ValueChanged" Margin="35,20,0,0" VerticalAlignment="Top" Width="200" Value="80" HorizontalAlignment="Left" Minimum="50" Maximum="100">
                                </Slider>
                                <NumberBox Minimum="50" Height="33" Width="120" HorizontalAlignment="Left" SpinButtonPlacementMode="Inline" SmallChange="1" LargeChange="10" x:Name="c7t" Margin="245,18,0,0"  ValueChanged="C2t_ValueChanged" GotFocus="C2t_FocusEngaged" LostFocus="C2t_FocusDisengaged" Value="{Binding ElementName=c7v, Path=Value, Mode=TwoWay}"></NumberBox>
                                <FontIcon FontFamily="{StaticResource SymbolThemeFontFamily}" x:Uid="Param_c1_Help" HorizontalAlignment="Left" Margin="355,7,0,0" Glyph="&#xe9ce;">
                                    <FontIcon.RenderTransform>
                                        <ScaleTransform ScaleX="0.7" ScaleY="0.7" />
                                    </FontIcon.RenderTransform>
                                </FontIcon>
                            </Grid>
                            <Grid Grid.Row="1" x:Name="Laptops_HTC_Temp_Desc" Grid.Column="1" HorizontalAlignment="Left" Margin="-600,0,0,0">
                                <TextBlock Text="                                    45(С)                         90(С)" Grid.Column="1" Margin="0,20,0,0" ></TextBlock>
                            </Grid>
                            <Grid Grid.Row="2" Grid.Column="0" HorizontalAlignment="Left" Width="2000">
                                <TextBlock x:Uid="Param_CPU_c2"  Text="Лимит CPU (W)" FontSize="15"></TextBlock>
                                <CheckBox  Margin="0,20,0,0" x:Name="c2" Checked="C2_Checked" Unchecked="C2_Checked"></CheckBox>
                                <Slider x:Name="c2v" ValueChanged="C2_ValueChanged" Margin="35,20,0,0" VerticalAlignment="Top" Width="200" HorizontalAlignment="Left" Minimum="1" Maximum="40" Value="20">
                                </Slider>
                                <NumberBox CornerRadius="6" Height="33" Width="120" Minimum="1" x:Name="c2t" HorizontalAlignment="Left" Margin="245,18,0,0"  ValueChanged="C2t_ValueChanged" GotFocus="C2t_FocusEngaged" LostFocus="C2t_FocusDisengaged"  SpinButtonPlacementMode="Inline" SmallChange="1" LargeChange="10" Value="{Binding ElementName=c2v, Path=Value, Mode=TwoWay}"/>
                                <FontIcon FontFamily="{StaticResource SymbolThemeFontFamily}" x:Uid="Param_c2_Help" HorizontalAlignment="Left" Margin="355,7,0,0" Glyph="&#xe9ce;">
                                    <FontIcon.RenderTransform>
                                        <ScaleTransform ScaleX="0.7" ScaleY="0.7" />
                                    </FontIcon.RenderTransform>
                                </FontIcon>
                            </Grid>
                            <Grid Grid.Row="2" Grid.Column="1" HorizontalAlignment="Left" Margin="-600,0,0,0">
                                <TextBlock Text="                                    20(W)                        30(W)" Grid.Column="1" Margin="0,20,0,0" ></TextBlock>
                            </Grid>
                            <Grid Grid.Row="3" x:Name="Laptops_Real_Wattage" Grid.Column="0" HorizontalAlignment="Left" Width="2000">
                                <TextBlock x:Uid="Param_CPU_c3" Text="Реальный CPU (W)" FontSize="15"></TextBlock>
                                <CheckBox  Margin="0,20,0,0" x:Name="c3" Checked="C3_Checked" Unchecked="C3_Checked"></CheckBox>
                                <Slider x:Name="c3v" ValueChanged="C3_ValueChanged" Margin="35,20,0,0" VerticalAlignment="Top" Width="200" HorizontalAlignment="Left" Minimum="1" Maximum="45" Value="25">
                                </Slider>
                                <NumberBox Minimum="1" Height="33" Width="120" HorizontalAlignment="Left" SpinButtonPlacementMode="Inline" SmallChange="1" LargeChange="10" x:Name="c3t" Margin="245,18,0,0"  ValueChanged="C2t_ValueChanged" GotFocus="C2t_FocusEngaged" LostFocus="C2t_FocusDisengaged" Value="{Binding ElementName=c3v, Path=Value, Mode=TwoWay}"></NumberBox>
                                <FontIcon FontFamily="{StaticResource SymbolThemeFontFamily}" x:Uid="Param_c3_Help" HorizontalAlignment="Left"  Margin="355,7,0,0" Glyph="&#xe9ce;">
                                    <FontIcon.RenderTransform>
                                        <ScaleTransform ScaleX="0.7" ScaleY="0.7" />
                                    </FontIcon.RenderTransform>
                                </FontIcon>
                            </Grid>
                            <Grid Grid.Row="3" x:Name="Laptops_Real_Wattage_Desc" Grid.Column="1" HorizontalAlignment="Left" Margin="-600,0,0,0">
                                <TextBlock Text="                                    25(W)                        35(W)" Grid.Column="1" Margin="0,20,0,0" ></TextBlock>
                            </Grid>
                            <Grid Grid.Row="4" x:Name="Laptops_Avg_Wattage" Grid.Column="0" HorizontalAlignment="Left" Width="2000">
                                <TextBlock x:Uid="Param_CPU_c4" Text="Средний CPU (W)" FontSize="15"></TextBlock>
                                <CheckBox  Margin="0,20,0,0" x:Name="c4" Checked="C4_Checked" Unchecked="C4_Checked"></CheckBox>
                                <Slider x:Name="c4v" ValueChanged="C4_ValueChanged" Margin="35,20,0,0" VerticalAlignment="Top" Width="200" HorizontalAlignment="Left" Minimum="1" Maximum="45" Value="25">
                                </Slider>
                                <NumberBox Minimum="1" Height="33" Width="120" HorizontalAlignment="Left" SpinButtonPlacementMode="Inline" SmallChange="1" LargeChange="10" x:Name="c4t" Margin="245,18,0,0"  ValueChanged="C2t_ValueChanged" GotFocus="C2t_FocusEngaged" LostFocus="C2t_FocusDisengaged" Value="{Binding ElementName=c4v, Path=Value, Mode=TwoWay}"></NumberBox>
                                <FontIcon FontFamily="{StaticResource SymbolThemeFontFamily}" x:Uid="Param_c4_Help" HorizontalAlignment="Left" Margin="355,7,0,0" Glyph="&#xe9ce;">
                                    <FontIcon.RenderTransform>
                                        <ScaleTransform ScaleX="0.7" ScaleY="0.7" />
                                    </FontIcon.RenderTransform>
                                </FontIcon>
                            </Grid>
                            <Grid Grid.Row="4" x:Name="Laptops_Avg_Wattage_Desc" Grid.Column="1" HorizontalAlignment="Left" Margin="-600,0,0,0">
                                <TextBlock Text="                                    25(W)                        35(W)" Grid.Column="1" Margin="0,20,0,0" ></TextBlock>
                            </Grid>
                            <Grid Grid.Row="5" x:Name="Laptops_Fast_Speed" Grid.Column="0" HorizontalAlignment="Left" Width="2000">
                                <TextBlock x:Uid="Param_CPU_c5" Text="Тик быстрого разгона (S)" FontSize="15"></TextBlock>
                                <CheckBox  Margin="0,20,0,0" x:Name="c5" Checked="C5_Checked" Unchecked="C5_Checked"></CheckBox>
                                <Slider x:Name="c5v" ValueChanged="C5_ValueChanged" Margin="35,20,0,0" VerticalAlignment="Top" Width="200" HorizontalAlignment="Left" Minimum="1" Maximum="128" Value="64">
                                </Slider>
                                <NumberBox Minimum="1" Height="33" Width="120" HorizontalAlignment="Left" SpinButtonPlacementMode="Inline" SmallChange="1" LargeChange="10" x:Name="c5t" Margin="245,18,0,0"  ValueChanged="C2t_ValueChanged" GotFocus="C2t_FocusEngaged" LostFocus="C2t_FocusDisengaged" Value="{Binding ElementName=c5v, Path=Value, Mode=TwoWay}"></NumberBox>
                                <FontIcon FontFamily="{StaticResource SymbolThemeFontFamily}" x:Uid="Param_c5_Help" HorizontalAlignment="Left" Margin="355,7,0,0" Glyph="&#xe7ba;">
                                    <FontIcon.RenderTransform>
                                        <ScaleTransform ScaleX="0.7" ScaleY="0.7" />
                                    </FontIcon.RenderTransform>
                                </FontIcon>
                            </Grid>
                            <Grid Grid.Row="5" x:Name="Laptops_Fast_Speed_Desc" Grid.Column="1" HorizontalAlignment="Left" Margin="-600,0,0,0">
                                <TextBlock Text="                                     64(S)                         48(S)" Grid.Column="1" Margin="0,20,0,0" ></TextBlock>
                            </Grid>
                            <Grid Grid.Row="6" x:Name="Laptops_slow_Speed" Grid.Column="0" HorizontalAlignment="Left" Width="2000">
                                <TextBlock x:Uid="Param_CPU_c6" Text="Тик медленного разгона (S)" FontSize="15"></TextBlock>
                                <CheckBox  Margin="0,20,0,0" x:Name="c6" Checked="C6_Checked" Unchecked="C6_Checked"></CheckBox>
                                <Slider x:Name="c6v" ValueChanged="C6_ValueChanged" Margin="35,20,0,0" VerticalAlignment="Top" Width="200" HorizontalAlignment="Left" Minimum="1" Maximum="128" Value="128">
                                </Slider>
                                <NumberBox Minimum="1" Height="33" Width="120" HorizontalAlignment="Left" SpinButtonPlacementMode="Inline" SmallChange="1" LargeChange="10" x:Name="c6t" Margin="245,18,0,0"  ValueChanged="C2t_ValueChanged" GotFocus="C2t_FocusEngaged" LostFocus="C2t_FocusDisengaged" Value="{Binding ElementName=c6v, Path=Value, Mode=TwoWay}"></NumberBox>
                                <FontIcon FontFamily="{StaticResource SymbolThemeFontFamily}" x:Uid="Param_c6_Help" HorizontalAlignment="Left" Margin="355,7,0,0" Glyph="&#xe7ba;">
                                    <FontIcon.RenderTransform>
                                        <ScaleTransform ScaleX="0.7" ScaleY="0.7" />
                                    </FontIcon.RenderTransform>
                                </FontIcon>
                            </Grid>
                            <Grid Grid.Row="6" x:Name="Laptops_slow_Speed_Desc" Grid.Column="1" HorizontalAlignment="Left" Margin="-600,0,0,0">
                                <TextBlock Text="                                    128(S)                       105(S)" Grid.Column="1" Margin="0,20,0,0" ></TextBlock>
                            </Grid> 
                        </Grid>
                    </Expander>

                    <Expander x:Name="Exp1" Grid.Row="1" VerticalAlignment="Top">
                        <Expander.Header>
                            <Grid Height="65" Width="2000">
                                <SymbolIcon Symbol="Library" HorizontalAlignment="Left" Margin="0,0,9,0"/>
                                <TextBlock x:Uid="Param_VRM_Name" Margin="35,12,0,0" FontWeight="SemiBold" VerticalAlignment="Top" Text="VRM Система питания CPU"></TextBlock>
                                <TextBlock Margin="35,30,0,0" VerticalAlignment="Top" FontWeight="Light" x:Uid="Param_VRM_Pdesc"  Text="Мощность процессора"></TextBlock>
                            </Grid>
                        </Expander.Header>
                        <Grid HorizontalAlignment="Left" Margin=" 30,0,0,0" Width="2000">
                            <!-- Монтируем секции -->
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <!-- От 0 до 6 -->
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>

                            <!-- Первая секция -->

                            <Grid Grid.Row="1" Grid.Column="0" HorizontalAlignment="Left" Width="2000">
                                <TextBlock x:Uid="Param_VRM_v2" Text="Лимит по току VRM (A)" FontSize="15"></TextBlock>
                                <CheckBox x:Name="V2" Checked="V2_Checked" Unchecked="V2_Checked" Margin="0,20,0,0"></CheckBox>
                                <Slider x:Name="V2V" ValueChanged="V2v_ValueChanged" Margin="35,20,0,0" VerticalAlignment="Top" Width="200" HorizontalAlignment="Left" Minimum="45" Maximum="180" Value="55">
                                </Slider>
                                <NumberBox Minimum="10" Height="33" Width="120" HorizontalAlignment="Left" SpinButtonPlacementMode="Inline" SmallChange="1" LargeChange="10" x:Name="v2t" Margin="245,18,0,0"  ValueChanged="C2t_ValueChanged" GotFocus="C2t_FocusEngaged" LostFocus="C2t_FocusDisengaged" Value="{Binding ElementName=V2V, Path=Value, Mode=TwoWay}"></NumberBox>
                                <FontIcon FontFamily="{StaticResource SymbolThemeFontFamily}" x:Uid="Param_v2_Help" HorizontalAlignment="Left" Margin="355,7,0,0" Glyph="&#xe9ce;">
                                    <FontIcon.RenderTransform>
                                        <ScaleTransform ScaleX="0.7" ScaleY="0.7" />
                                    </FontIcon.RenderTransform>
                                </FontIcon>
                            </Grid>
                            <Grid Grid.Row="1" Grid.Column="1" HorizontalAlignment="Left" Margin="-600,0,0,0">
                                <TextBlock Text="                                    55(A)                         70(A)" Grid.Column="1" Margin="0,20,0,0" ></TextBlock>
                            </Grid>
                            <Grid Grid.Row="2" Grid.Column="0" x:Name="VRM_Laptops_SoC_Max" HorizontalAlignment="Left" Width="2000">
                                <TextBlock x:Uid="Param_VRM_v3" Text="Максимальный ток SoC (A)" FontSize="15"></TextBlock>
                                <CheckBox Checked="V3_Checked" Unchecked="V3_Checked" x:Name="V3" Margin="0,20,0,0"></CheckBox>
                                <Slider x:Name="V3V" ValueChanged="V3v_ValueChanged" Margin="35,20,0,0" VerticalAlignment="Top" Width="200" HorizontalAlignment="Left" Minimum="13" Maximum="180">
                                </Slider>
                                <NumberBox Minimum="5" Height="33" Width="120" HorizontalAlignment="Left" SpinButtonPlacementMode="Inline" SmallChange="1" LargeChange="10" x:Name="v3t" Margin="245,18,0,0"  ValueChanged="C2t_ValueChanged" GotFocus="C2t_FocusEngaged" LostFocus="C2t_FocusDisengaged" Value="{Binding ElementName=V3V, Path=Value, Mode=TwoWay}"></NumberBox>
                                <FontIcon FontFamily="{StaticResource SymbolThemeFontFamily}" x:Uid="Param_v3_Help" HorizontalAlignment="Left" Margin="355,7,0,0" Glyph="&#xe9ce;">
                                    <FontIcon.RenderTransform>
                                        <ScaleTransform ScaleX="0.7" ScaleY="0.7" />
                                    </FontIcon.RenderTransform>

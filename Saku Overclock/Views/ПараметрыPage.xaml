<Page
    x:Class="Saku_Overclock.Views.ПараметрыPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <Page.Resources>
        <ThemeShadow x:Name="SharedShadow"/>
        <Flyout x:Name="SymbolFlyout" Placement="Bottom">
            <!-- flyout content -->
            <GridView ItemClick="SymbolList_ItemClick" IsItemClickEnabled="True">
                <GridView.ItemsPanel>
                    <ItemsPanelTemplate>
                        <ItemsWrapGrid MaximumRowsOrColumns="5" Orientation="Horizontal"/>
                    </ItemsPanelTemplate>
                </GridView.ItemsPanel>
                <GridView.Resources>
                    <Style TargetType="Rectangle">
                        <Setter Property="Width" Value="40"/>
                        <Setter Property="Height" Value="40"/>
                        <Setter Property="RadiusX" Value="4"/>
                        <Setter Property="RadiusY" Value="4"/>
                    </Style>
                </GridView.Resources>
                <GridView.Items>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xEcad;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xeafc;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xeb05;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xeadf;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xe734;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xeadf;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xeb3c;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xeb44;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xeb51;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xebd2;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xec32;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xec46;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xec48;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xec49;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xec4a;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xecc6;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xe7ba;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xedad;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xee6f;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xee71;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xf211;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xe945;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xe9d9;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xe9ca;"/>
                    <FontIcon VerticalAlignment="Center" Margin="0,-10,0,0" HorizontalAlignment="Left" Glyph="&#xe9ce;"/>
                </GridView.Items>
            </GridView>
        </Flyout>
    </Page.Resources>
    <Grid Margin="-48,0,-28,0">
        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" MaxWidth="1200">
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition Height="62"/>
            </Grid.RowDefinitions>
            <!-- Начало -->
            <TextBlock x:Uid="Param_Name" x:Name="ParamName" Text="Разгон" FontSize="30" Margin="0,-95,0,0"/>
            <StackPanel Grid.Row="0" x:Name="SuggestionsFilterStackPanel" HorizontalAlignment="Center"
                        VerticalAlignment="Top" Orientation="Horizontal" Margin="0,-90,-11,0">
                <AutoSuggestBox AutoMaximizeSuggestionArea="True" BorderThickness="0" Loaded="SuggestBox_Loaded"
                                CornerRadius="16" Shadow="{StaticResource SharedShadow}" Translation="0,0,20"
                                DisplayMemberPath="Text" x:Uid="Param_SuggestBox" x:Name="SuggestBox"
                                TextChanged="SuggestBox_OnTextChanged" Margin="10,0,0,0" Width="400" QueryIcon="Find"
                                Height="40" PlaceholderText="Поиск..."/>
                <Button CornerRadius="14" Shadow="{StaticResource SharedShadow}" Translation="0,0,20"
                        x:Name="FiltersButton" Margin="10,0,0,0" Height="40" Width="40">
                    <FontIcon Margin="-5,0,-5,0" Glyph="&#xE71C;"/>
                    <Button.Flyout>
                        <Flyout>
                            <StackPanel Orientation="Vertical">
                                <TextBlock x:Uid="Param_Filters_By" Text="Сортировка по:" FontWeight="SemiBold"/>
                                <ToggleButton CornerRadius="12" x:Name="FilterButtonsTemp"
                                              Checked="FilterButton_Checked" Unchecked="FilterButton_Checked"
                                              Margin="0,8,0,0">
                                    <StackPanel Orientation="Horizontal">
                                        <Border Height="20" Width="20" CornerRadius="15">
                                            <FontIcon Glyph="&#xE9CA;" FontSize="15"/>
                                            <Border.Background>
                                                <LinearGradientBrush StartPoint="0.3,0.5" EndPoint="1.3,1.5">
                                                    <GradientStop Color="#AAADD8E6" Offset="0.0"/>
                                                    <GradientStop Color="#AAFFC0CB" Offset="0.10"/>
                                                </LinearGradientBrush>
                                            </Border.Background>
                                        </Border>
                                        <TextBlock Margin="5,0,0,0" x:Uid="Param_Filters_By_Temp" Text="Температура"/>
                                    </StackPanel>
                                </ToggleButton>
                                <ToggleButton CornerRadius="12" x:Name="FilterButtonsPower"
                                              Checked="FilterButton_Checked" Unchecked="FilterButton_Checked"
                                              Margin="0,7,0,0">
                                    <StackPanel Orientation="Horizontal">
                                        <Border Height="20" Width="20" CornerRadius="15">
                                            <FontIcon Glyph="&#xE945;" FontSize="15"/>
                                            <Border.Background>
                                                <LinearGradientBrush StartPoint="0.3,0.5" EndPoint="1.3,1.5">
                                                    <GradientStop Color="#AAfdb640" Offset="0.0"/>
                                                    <GradientStop Color="#AAbc7838" Offset="0.10"/>
                                                </LinearGradientBrush>
                                            </Border.Background>
                                        </Border>
                                        <TextBlock Margin="5,0,0,0" x:Uid="Param_Filters_By_Power"
                                                   Text="Мощность и напряжение"/>
                                    </StackPanel>
                                </ToggleButton>
                                <ToggleButton CornerRadius="12" x:Name="FilterButtonsCurrent"
                                              Checked="FilterButton_Checked" Unchecked="FilterButton_Checked"
                                              Margin="0,7,0,0">
                                    <StackPanel Orientation="Horizontal">
                                        <Border Height="20" Width="20" CornerRadius="15">
                                            <FontIcon Glyph="&#xF7B0;" FontSize="15"/>
                                            <Border.Background>
                                                <LinearGradientBrush StartPoint="0.3,0.5" EndPoint="1.3,1.5">
                                                    <GradientStop Color="#AAb3100c" Offset="0.0"/>
                                                    <GradientStop Color="#AAbf80ff" Offset="0.10"/>
                                                </LinearGradientBrush>
                                            </Border.Background>
                                        </Border>
                                        <TextBlock Margin="5,0,0,0" x:Uid="Param_Filters_By_Current" Text="Ток"/>
                                    </StackPanel>
                                </ToggleButton>
                                <ToggleButton CornerRadius="12" x:Name="FilterButtonsFreq"
                                              Checked="FilterButton_Checked" Unchecked="FilterButton_Checked"
                                              Margin="0,7,0,0">
                                    <StackPanel Orientation="Horizontal">
                                        <Border Height="20" Width="20" CornerRadius="15">
                                            <FontIcon Glyph="&#xEC4A;" FontSize="15"/>
                                            <Border.Background>
                                                <LinearGradientBrush StartPoint="0.3,0.5" EndPoint="1.3,1.5">
                                                    <GradientStop Color="#AA4de2d1" Offset="0.0"/>
                                                    <GradientStop Color="#AA09a22b" Offset="0.10"/>
                                                </LinearGradientBrush>
                                            </Border.Background>
                                        </Border>
                                        <TextBlock Margin="5,0,0,0" x:Uid="Param_Filters_By_Freq" Text="Частота"/>
                                    </StackPanel>
                                </ToggleButton>
                                <ToggleButton CornerRadius="12" x:Name="FilterButtonsTime"
                                              Checked="FilterButton_Checked" Unchecked="FilterButton_Checked"
                                              Margin="0,7,0,0">
                                    <StackPanel Orientation="Horizontal">
                                        <Border Height="20" Width="20" CornerRadius="15">
                                            <FontIcon Glyph="&#xE916;" FontSize="15"/>
                                            <Border.Background>
                                                <LinearGradientBrush StartPoint="0.3,0.5" EndPoint="1.3,1.5">
                                                    <GradientStop Color="#AAa217e3" Offset="0.0"/>
                                                    <GradientStop Color="#AA2f2f70" Offset="0.10"/>
                                                </LinearGradientBrush>
                                            </Border.Background>
                                        </Border>
                                        <TextBlock Margin="5,0,0,0" x:Uid="Param_Filters_By_Time" Text="Время"/>
                                    </StackPanel>
                                </ToggleButton>
                                <ToggleButton CornerRadius="12" x:Name="FilterButtonsOther"
                                              Checked="FilterButton_Checked" Unchecked="FilterButton_Checked"
                                              Margin="0,7,0,0">
                                    <StackPanel Orientation="Horizontal">
                                        <Border Height="20" Width="20" CornerRadius="15">
                                            <FontIcon Glyph="&#xE712;" FontSize="15"/>
                                            <Border.Background>
                                                <LinearGradientBrush StartPoint="0.3,0.5" EndPoint="1.3,1.5">
                                                    <GradientStop Color="#AAb41a7d" Offset="0.0"/>
                                                    <GradientStop Color="#AAe7cb35" Offset="0.10"/>
                                                </LinearGradientBrush>
                                            </Border.Background>
                                        </Border>
                                        <TextBlock Margin="5,0,0,0" x:Uid="Param_Filters_By_Others"
                                                   Text="Другие функции"/>
                                    </StackPanel>
                                </ToggleButton>
                                <ToggleButton CornerRadius="12" Visibility="Collapsed" x:Name="FilterButtonsHide"
                                              Checked="FilterButton_Checked" Unchecked="FilterButton_Checked"
                                              Margin="0,7,0,0">
                                    <StackPanel Orientation="Horizontal">
                                        <Border Height="20" Width="20" CornerRadius="15">
                                            <FontIcon Glyph="&#xE7B3;" FontSize="15"/>
                                            <Border.Background>
                                                <LinearGradientBrush StartPoint="0.3,0.5" EndPoint="1.3,1.5">
                                                    <GradientStop Color="#AAABC36F" Offset="0.0"/>
                                                    <GradientStop Color="#AAC2F855" Offset="0.10"/>
                                                </LinearGradientBrush>
                                            </Border.Background>
                                        </Border>
                                        <TextBlock Margin="5,0,0,0" x:Uid="Param_Filters_By_Hide"
                                                   Text="Скрытые параметры"/>
                                    </StackPanel>
                                </ToggleButton>
                                <Button Margin="0,14,0,0" Width="200" HorizontalAlignment="Left"
                                        Click="FilterButtons_ResetButton_Click"
                                        Style="{ThemeResource AccentButtonStyle}" CornerRadius="14">
                                    <StackPanel Orientation="Horizontal">
                                        <FontIcon Margin="-5,0,-5,0" Glyph="&#xE72C;"/>
                                        <TextBlock Margin="10,0,0,0" x:Uid="Param_Filters_By_Reset"
                                                   Text="Сбросить фильтры" FontWeight="Bold"/>
                                    </StackPanel>
                                </Button>
                            </StackPanel>
                        </Flyout>
                    </Button.Flyout>
                </Button>
            </StackPanel>
            <Grid Grid.Row="0" Margin="0,-90,-11,0" HorizontalAlignment="Right" x:Name="ProfilesGrid">
                <Border CornerRadius="16" VerticalAlignment="Top" Height="40" Width="200" HorizontalAlignment="Right"
                        Shadow="{StaticResource SharedShadow}" Translation="0,0,20"/>
                <ComboBox CornerRadius="16" HorizontalAlignment="Right" Style="{ThemeResource AcrylicComboBoxStyle}"
                          Height="40" Width="200" x:Name="ProfileCom" SelectionChanged="ProfileCOM_SelectionChanged">
                    <ComboBox x:Uid="Param_PremadedCombo" IsEnabled="False"/>
                </ComboBox>
            </Grid>

            <!-- Середина -->

            <ScrollViewer Grid.Row="0" Padding="0,-30,10,0" IsTabStop="False"
                          UseSystemFocusVisuals="False" x:Name="MainScroll"
                          VerticalScrollBarVisibility="Auto"
                          VerticalScrollMode="Auto" Margin="0,-10,-22,0">
                <Grid>
                    <Grid VerticalAlignment="Top" x:Name="NormalUserMode" Visibility="Visible">
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <Grid CornerRadius="24" Shadow="{StaticResource SharedShadow}" Translation="0,0,12"
                              VerticalAlignment="Top">
                            <Expander CornerRadius="24" IsExpanded="False" VerticalAlignment="Stretch"
                                      HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch"
                                      Background="{ThemeResource ControlAltFillColorSecondaryBrush}">
                                <Expander.Header>
                                    <Grid Height="65">
                                        <SymbolIcon Symbol="SelectAll" HorizontalAlignment="Left" Margin="0,0,9,0"/>
                                        <TextBlock Margin="35,12,0,0" FontWeight="SemiBold" VerticalAlignment="Top"
                                                   x:Uid="Param_CPU_Text" Text="Мощность процессора"/>
                                        <TextBlock x:Name="TextColor" Margin="35,30,0,0" FontWeight="Light"
                                                   VerticalAlignment="Top" x:Uid="Param_CPU_Pdesc"
                                                   Text="Мощность процессора"/>
                                    </Grid>
                                </Expander.Header>
                                <Grid HorizontalAlignment="Stretch" Margin="0,0,0,0">
                                    <!-- Монтируем секции -->
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="7*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>

                                    <!-- Первая секция -->
                                    <StackPanel Padding="0,0,0,13" Orientation="Vertical" HorizontalAlignment="Stretch">
                                        <StackPanel Orientation="Horizontal">
                                            <Border Height="20" Width="20" CornerRadius="15">
                                                <FontIcon Glyph="&#xE9CA;" FontSize="15"/>
                                                <Border.Background>
                                                    <LinearGradientBrush StartPoint="0.3,0.5" EndPoint="1.3,1.5">
                                                        <GradientStop Color="#AAADD8E6" Offset="0.0"/>
                                                        <GradientStop Color="#AAFFC0CB" Offset="0.10"/>
                                                    </LinearGradientBrush>
                                                </Border.Background>
                                            </Border>
                                            <TextBlock x:Uid="Param_CPU_c1" Margin="5,0,0,0"
                                                       Text="Максимальная температура CPU" FontSize="15"/>
                                            <FontIcon x:Uid="Param_c1_Help" HorizontalAlignment="Left"
                                                      Margin="5,4,0,-4" Glyph="&#xe9ce;">
                                                <FontIcon.RenderTransform>
                                                    <ScaleTransform ScaleX="0.7" ScaleY="0.7"/>
                                                </FontIcon.RenderTransform>
                                            </FontIcon>
                                        </StackPanel>
                                        <StackPanel Margin="0,7,0,0" Orientation="Horizontal"
                                                    HorizontalAlignment="Left">
                                            <Grid>
                                                <CheckBox Width="10" x:Name="C1" Checked="C1_Checked"
                                                          Unchecked="C1_Checked"/>
                                                <Slider Style="{ThemeResource FatSliderStyle}" x:Name="C1V"
                                                        ValueChanged="C1_ValueChanged" Margin="40,0,0,0"
                                                        VerticalAlignment="Top" Width="250" Value="80"
                                                        HorizontalAlignment="Left" Minimum="50" Maximum="100"/>
                                            </Grid>
                                            <Grid Margin="23,0,0,0" HorizontalAlignment="Left" CornerRadius="10"
                                                  Shadow="{ThemeResource SharedShadow}" Translation="0,0,10">
                                                <NumberBox Foreground="{ThemeResource TextFillColorTertiary}"
                                                           CornerRadius="10" Minimum="50" Height="33" Width="120"
                                                           SpinButtonPlacementMode="Inline" SmallChange="1"
                                                           LargeChange="10" Tag="C1V"
                                                           ValueChanged="TargetNumberBox_ValueChanged"
                                                           GotFocus="TargetNumberBox_FocusEngaged"
                                                           LostFocus="TargetNumberBox_FocusDisengaged"
                                                           Value="{Binding ElementName=C1V, Path=Value, Mode=TwoWay}"/>
                                            </Grid>
                                        </StackPanel>
                                    </StackPanel>
                                    <Grid Grid.Row="0" Grid.Column="1" HorizontalAlignment="Center"
                                          VerticalAlignment="Top">
                                        <StackPanel Margin="0,-5,0,0" Height="32" Orientation="Horizontal"
                                                    VerticalAlignment="Top" CornerRadius="12" Translation="0,0,10"
                                                    Background="{ThemeResource CardBackgroundFillColorSecondaryBrush}"
                                                    Shadow="{ThemeResource SharedShadow}">
                                            <TextBlock Width="53" Margin="15,6,0,0" x:Uid="Param_Suggestion"
                                                       Text="Совет:"/>
                                            <StackPanel Margin="15,1,0,1" Orientation="Horizontal" CornerRadius="14"
                                                        Background="{ThemeResource SystemFillColorSuccessBackgroundBrush}">
                                                <FontIcon Margin="8,5,0,0" VerticalAlignment="Top" Glyph="&#xEC49;"/>
                                                <TextBlock Margin="5,6,8,0" Width="47"
                                                           Foreground="{ThemeResource TextFillColorSecondary}"
                                                           x:Uid="Param_Suggestion_Balance" Text="Баланс"/>
                                            </StackPanel>
                                            <StackPanel Margin="10,1,15,1" Orientation="Horizontal" CornerRadius="14"
                                                        Background="{ThemeResource SystemFillColorCriticalBackgroundBrush}">
                                                <FontIcon Margin="8,5,0,0" VerticalAlignment="Top" Glyph="&#xECAD;"/>
                                                <TextBlock Margin="5,6,8,0" Width="77"
                                                           Foreground="{ThemeResource TextFillColorSecondary}"
                                                           x:Uid="Param_Suggestion_Performance" Text="Скорость"/>
                                            </StackPanel>
                                        </StackPanel>
                                        <StackPanel Margin="0,31,0,0" Height="28" Orientation="Horizontal"
                                                    VerticalAlignment="Top">
                                            <Grid Margin="83,1,0,1" Width="89">
                                                <StackPanel HorizontalAlignment="Center" VerticalAlignment="Stretch"
                                                            CornerRadius="10" Translation="0,0,12"
                                                            Background="{ThemeResource ControlStrokeColorOnAccentDefaultBrush}"
                                                            Shadow="{ThemeResource SharedShadow}">
                                                    <TextBlock Margin="8,4,8,0"
                                                               Foreground="{ThemeResource TextFillColorSecondary}"
                                                               x:Name="TempRecommend0" Text="80C"/>
                                                </StackPanel>
                                            </Grid>
                                            <Grid Margin="10,1,15,1" Width="117">
                                                <StackPanel HorizontalAlignment="Center" VerticalAlignment="Stretch"
                                                            CornerRadius="10" Translation="0,0,12"
                                                            Background="{ThemeResource ControlStrokeColorOnAccentDefaultBrush}"
                                                            Shadow="{ThemeResource SharedShadow}">
                                                    <TextBlock Margin="8,4,8,0"
                                                               Foreground="{ThemeResource TextFillColorSecondary}"
                                                               x:Name="TempRecommend1" Text="90C"/>
                                                </StackPanel>
                                            </Grid>
                                        </StackPanel>
                                    </Grid>

                                    <StackPanel Padding="0,0,0,10" Grid.Row="1" Grid.Column="0"
                                                HorizontalAlignment="Left" x:Name="LaptopsStapmLimit">
                                        <StackPanel Orientation="Horizontal">
                                            <Border Height="20" Width="20" CornerRadius="15">
                                                <FontIcon Glyph="&#xE945;" FontSize="15"/>
                                                <Border.Background>
                                                    <LinearGradientBrush StartPoint="0.3,0.5" EndPoint="1.3,1.5">
                                                        <GradientStop Color="#AAfdb640" Offset="0.0"/>
                                                        <GradientStop Color="#AAbc7838" Offset="0.10"/>
                                                    </LinearGradientBrush>
                                                </Border.Background>
                                            </Border>
                                            <TextBlock Margin="5,0,0,0" x:Uid="Param_CPU_c2" Text="Лимит мощности CPU"
                                                       FontSize="15"/>
                                            <FontIcon Margin="5,4,0,-4" x:Uid="Param_c2_Help"
                                                      HorizontalAlignment="Left" Glyph="&#xe9ce;">
                                                <FontIcon.RenderTransform>
                                                    <ScaleTransform ScaleX="0.7" ScaleY="0.7"/>
                                                </FontIcon.RenderTransform>
                                            </FontIcon>
                                        </StackPanel>
                                        <StackPanel Margin="0,7,0,0" Orientation="Horizontal">
                                            <Grid>
                                                <CheckBox x:Name="C2" Checked="C2_Checked" Unchecked="C2_Checked"/>
                                                <Slider Style="{ThemeResource FatSliderStyle}" x:Name="C2V"
                                                        ValueChanged="C2_ValueChanged" Margin="40,0,0,0"
                                                        VerticalAlignment="Top" Width="250" HorizontalAlignment="Left"
                                                        Minimum="1" Maximum="40" Value="20"/>
                                            </Grid>
                                            <Grid Margin="23,0,0,0" HorizontalAlignment="Left" CornerRadius="10"
                                                  Shadow="{ThemeResource SharedShadow}" Translation="0,0,10">
                                                <NumberBox Foreground="{ThemeResource TextFillColorTertiary}"
                                                           CornerRadius="10" Height="33" Width="120" Minimum="1"
                                                           ValueChanged="TargetNumberBox_ValueChanged"
                                                           GotFocus="TargetNumberBox_FocusEngaged"
                                                           LostFocus="TargetNumberBox_FocusDisengaged"
                                                           SpinButtonPlacementMode="Inline" SmallChange="1"
                                                           LargeChange="10" Tag="C2V"
                                                           Value="{Binding ElementName=C2V, Path=Value, Mode=TwoWay}"/>
                                            </Grid>
                                        </StackPanel>
                                    </StackPanel>
                                    <Grid Grid.Row="1" Grid.Column="1" HorizontalAlignment="Center"
                                          x:Name="LaptopsStapmLimitDesc">
                                        <StackPanel Margin="0,31,0,0" Height="28" Orientation="Horizontal"
                                                    VerticalAlignment="Top">
                                            <Grid Margin="83,1,0,1" Width="89">
                                                <StackPanel HorizontalAlignment="Center" VerticalAlignment="Stretch"
                                                            CornerRadius="10" Translation="0,0,12"
                                                            Background="{ThemeResource ControlStrokeColorOnAccentDefaultBrush}"
                                                            Shadow="{ThemeResource SharedShadow}">
                                                    <TextBlock Margin="8,4,8,0"
                                                               Foreground="{ThemeResource TextFillColorSecondary}"
                                                               x:Name="StapmRecommend0" Text="20W"/>
                                                </StackPanel>
                                            </Grid>
                                            <Grid Margin="10,1,15,1" Width="117">
                                                <StackPanel HorizontalAlignment="Center" VerticalAlignment="Stretch"
                                                            CornerRadius="10" Translation="0,0,12"
                                                            Background="{ThemeResource ControlStrokeColorOnAccentDefaultBrush}"
                                                            Shadow="{ThemeResource SharedShadow}">
                                                    <TextBlock Margin="8,4,8,0"
                                                               Foreground="{ThemeResource TextFillColorSecondary}"
                                                               x:Name="StapmRecommend1" Text="30W"/>
                                                </StackPanel>
                                            </Grid>
                                        </StackPanel>
                                    </Grid>

                                    <StackPanel Padding="0,0,0,10" Grid.Row="2" Grid.Column="0"
                                                x:Name="AdvLaptopAplusALimit">
                                        <StackPanel Orientation="Horizontal">
                                            <Border Height="20" Width="20" CornerRadius="15">
                                                <FontIcon Glyph="&#xE945;" FontSize="15"/>
                                                <Border.Background>
                                                    <LinearGradientBrush StartPoint="0.3,0.5" EndPoint="1.3,1.5">
                                                        <GradientStop Color="#AAfdb640" Offset="0.0"/>
                                                        <GradientStop Color="#AAbc7838" Offset="0.10"/>
                                                    </LinearGradientBrush>
                                                </Border.Background>
                                            </Border>
                                            <TextBlock Margin="5,0,0,0" x:Uid="Param_ADV_a9"
                                                       Text="Общий лимит потребления (W)" FontSize="15"/>
                                            <FontIcon x:Uid="Param_a9_Help" HorizontalAlignment="Left"
                                                      Margin="5,4,0,-4" Glyph="&#xe9ce;">
                                                <FontIcon.RenderTransform>
                                                    <ScaleTransform ScaleX="0.7" ScaleY="0.7"/>
                                                </FontIcon.RenderTransform>
                                            </FontIcon>
                                        </StackPanel>
                                        <StackPanel Margin="0,7,0,0" Orientation="Horizontal">
                                            <Grid>
                                                <CheckBox Checked="A9_Checked" Unchecked="A9_Checked" x:Name="A9"/>
                                                <Slider Style="{ThemeResource FatSliderStyle}" x:Name="A9V"
                                                        ValueChanged="A9v_ValueChanged" Margin="40,0,0,0"
                                                        VerticalAlignment="Top" Width="250" HorizontalAlignment="Left"
                                                        Minimum="10" Maximum="200" Value="45"/>
                                            </Grid>
                                            <Grid Margin="23,0,0,0" HorizontalAlignment="Left" CornerRadius="10"
                                                  Shadow="{ThemeResource SharedShadow}" Translation="0,0,10">
                                                <NumberBox Foreground="{ThemeResource TextFillColorTertiary}"
                                                           Minimum="10" Height="33" CornerRadius="10" Width="120"
                                                           SpinButtonPlacementMode="Inline" SmallChange="1"
                                                           LargeChange="10" Tag="A9V"
                                                           ValueChanged="TargetNumberBox_ValueChanged"

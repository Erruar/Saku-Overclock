                        <TextBlock x:Uid="Cooler_Current_Fan" x:Name="Fan1Cur" Text="Текущая скорость: 25 %" Margin="0,90,0,0"/>
                        <ProgressBar x:Name="Fan1Pr" Value="25" ValueChanged="Fan1Pr_ValueChanged" HorizontalAlignment="Left" Width="200" Margin="0,-50,0,0"></ProgressBar>
